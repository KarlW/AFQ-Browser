#!/usr/bin/env python

from optparse import OptionParser
import afqbrowser as afqb

usage = """afqbrowser-run [options]"""
parser = OptionParser(usage)

parser.add_option("-t", "--target", type="string", help="Target", default=None)
parser.add_option("-p", "--port", type="int", help="Port", default=8080)


opts, args = parser.parse_args()

afqb.run(opts.target, opts.port)
